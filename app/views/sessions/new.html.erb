<div class="login">
	<div class="container">
		<div class="form">
			<h1>Log In</h1>
			<%=form_for(:session,url: login_path) do |f|%>
				<%= f.email_field :email, :placeholder => "Email" %> <br>
			    <%= f.password_field :password, :placeholder => "Password" %> <br>
			    <%= f.submit "Log in", class: "btn-submit" %>
			<% end %>
			<p>Not logged in? <%=link_to "Sign Up",new_user_path%>
 		</div>
	</div>
</div>