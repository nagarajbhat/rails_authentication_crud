<div class="col-md-4"></div>
<div class="col-md-4">
		<h1>Log In</h1>
		<%=form_for(:session,url: login_path) do |f|%>
			
		
			<%= f.email_field :email, :placeholder => "Email",class:"formboxes" %> <br>
		    <%= f.password_field :password, :placeholder => "Password",class:"formboxes" %> <br>
		    <%= f.submit "Log in", class: "btn btn-success" %>
				
		<% end %>

		<p>Don't have an account? <%=link_to "Sign Up",new_user_path%>
 		
</div>
<div class="col-md-4"></div>
			